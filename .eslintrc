{
  "root": true,
  "env": {
    "node": true
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "rules": {
    "no-console": [
      1,
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "no-new-object": 2,
    "no-array-constructor": 2,
    "no-new-func": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "no-prototype-builtins": 2,
    "no-eval": 2,
    "prefer-spread": 2,
    "no-unused-vars": 1,
    "no-param-reassign": 2,
    "prefer-arrow-callback": 2,
    "no-confusing-arrow": 2,
    "no-useless-constructor": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-undef": 2,
    "prefer-const": 2,
    "no-multi-assign": 2,
    "no-plusplus": 2,
    "eqeqeq": 2,
    "no-unneeded-ternary": 2,
    "nonblock-statement-body-position": 2,
    "no-else-return": 2,
    "no-nested-ternary": 2,
    "no-loop-func": 2
  }
}
